{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "WS-ADF-PROYECTO-FINAL-ANGIE-UBAQUE"
		},
		"AKVFile_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KV-FILE-ANGIE.vault.azure.net/"
		},
		"ls_adls_proyecto_Angie_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsangieubaque.dfs.core.windows.net/"
		},
		"ls_fs_proyectoAngieUbaque_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\data_adf"
		},
		"ls_fs_proyectoAngieUbaque_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Angie"
		},
		"TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_NOMINA": {
			"type": "string",
			"defaultValue": "CoE12_Nomina.xlsx"
		},
		"TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_PROYECCION": {
			"type": "string",
			"defaultValue": "Proyeccion.xlsx"
		},
		"TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_CCR": {
			"type": "string",
			"defaultValue": "Report_UtilizacionTimeTracker.xlsx"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "WS-ADF-PROYECTO-FINAL-ANGIE-UBAQUE_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AKVFile_properties_typeProperties_baseUrl": {
						"value": "[parameters('AKVFile_properties_typeProperties_baseUrl')]"
					},
					"ls_adls_proyecto_Angie_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_proyecto_Angie_properties_typeProperties_url')]"
					},
					"ls_fs_proyectoAngieUbaque_properties_typeProperties_host": {
						"value": "[parameters('ls_fs_proyectoAngieUbaque_properties_typeProperties_host')]"
					},
					"ls_fs_proyectoAngieUbaque_properties_typeProperties_userId": {
						"value": "[parameters('ls_fs_proyectoAngieUbaque_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "WS-ADF-PROYECTO-FINAL-ANGIE-UBAQUE_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_NOMINA": {
						"value": "[parameters('TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_NOMINA')]"
					},
					"TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_PROYECCION": {
						"value": "[parameters('TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_PROYECCION')]"
					},
					"TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_CCR": {
						"value": "[parameters('TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_CCR')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'WS-ADF-PROYECTO-FINAL-ANGIE-UBAQUE_ArmTemplate_0')]"
			]
		}
	]
}