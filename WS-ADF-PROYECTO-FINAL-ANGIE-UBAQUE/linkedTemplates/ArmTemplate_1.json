{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "WS-ADF-PROYECTO-FINAL-ANGIE-UBAQUE"
		},
		"TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_NOMINA": {
			"type": "string",
			"defaultValue": "CoE12_Nomina.xlsx"
		},
		"TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_PROYECCION": {
			"type": "string",
			"defaultValue": "Proyeccion.xlsx"
		},
		"TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_CCR": {
			"type": "string",
			"defaultValue": "Report_UtilizacionTimeTracker.xlsx"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/TR_EJECUTAR_AUTOMATICO')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Stopped",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "PL_MAESTRO",
							"type": "PipelineReference"
						},
						"parameters": {
							"NOMBRE_ARCHIVO_NOMINA": "[parameters('TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_NOMINA')]",
							"NOMBRE_ARCHIVO_PROYECCION": "[parameters('TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_PROYECCION')]",
							"NOMBRE_ARCHIVO_CCR": "[parameters('TR_EJECUTAR_AUTOMATICO_properties_PL_MAESTRO_parameters_NOMBRE_ARCHIVO_CCR')]"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Day",
						"interval": 365,
						"startTime": "2023-08-28T17:30:00",
						"endTime": "2023-08-28T23:40:32",
						"timeZone": "SA Pacific Standard Time",
						"schedule": {}
					}
				}
			},
			"dependsOn": []
		}
	]
}